{
  "name": "oclif-plugin-update-notifier",
  "description": "update-notifier for oclif plugins",
  "version": "1.5.39",
  "author": "jayree",
  "type": "module",
  "bugs": "https://github.com/jayree/oclif-plugin-update-notifier/issues",
  "dependencies": {
    "@jayree/changelog": "^1.1.5",
    "@oclif/core": "^2.10.0",
    "@salesforce/kit": "^3.0.9",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "got": "^13.0.0",
    "package-json": "^8.1.1",
    "semver": "^7.5.3",
    "semver-diff": "^4.0.0",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^4.0.12",
    "@oclif/plugin-help": "^5.2.17",
    "@oclif/test": "^2.4.5",
    "@salesforce/dev-config": "^4.0.1",
    "@salesforce/dev-scripts": "^5.7.0",
    "@salesforce/prettier-config": "^0.0.3",
    "@types/boxen": "^3.0.1",
    "@types/debug": "^4.1.8",
    "@types/fs-extra": "^11.0.1",
    "@types/semver": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "c8": "^8.0.1",
    "chai": "^4.3.7",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-salesforce": "^2.0.2",
    "eslint-config-salesforce-typescript": "^1.1.2",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sf-plugin": "^1.15.6",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "oclif": "^3.11.3",
    "patch-package": "^8.0.0",
    "pinst": "^3.0.0",
    "prettier": "^3.0.1",
    "pretty-quick": "^3.1.3",
    "sinon": "^15.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "wireit": "^0.10.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/CHANGELOG.md"
  ],
  "homepage": "https://github.com/jayree/oclif-plugin-update-notifier",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "topics": {
      "plugins": {
        "external": true,
        "update": {
          "external": true
        }
      }
    },
    "hooks": {
      "prerun": "./lib/hooks/prerun",
      "updatenotifier": "./lib/hooks/updatenotifier",
      "update": "./lib/hooks/changelog"
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot"
    ],
    "info": {
      "releasenotes": {
        "distTagUrl": "https://registry.npmjs.org/-/package/oclif-plugin-update-notifier/dist-tags",
        "releaseNotesPath": "https://github.com/jayree/oclif-plugin-update-notifier/tree/main",
        "releaseNotesFilename": "CHANGELOG.md"
      }
    }
  },
  "repository": "jayree/oclif-plugin-update-notifier",
  "scripts": {
    "build": "wireit",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "wireit",
    "format": "wireit",
    "lint": "wireit",
    "prepack": "sf-prepack",
    "prepare": "patch-package && sf-install",
    "test": "wireit",
    "version": "oclif readme --no-aliases"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "wireit": {
    "build": {
      "dependencies": [
        "compile",
        "lint"
      ]
    },
    "compile": {
      "command": "tsc --build --pretty",
      "files": [
        "src/**/*.ts",
        "**/tsconfig.json",
        "messages/**"
      ],
      "output": [
        "lib/**",
        "*.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    },
    "format": {
      "command": "prettier --write \"+(src|test|schemas)/**/*.+(ts|js|json)|command-snapshot.json\"",
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        "schemas/**/*.json",
        "command-snapshot.json",
        ".prettier*"
      ],
      "output": []
    },
    "lint": {
      "command": "eslint --color --cache --cache-location .eslintcache .",
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        "messages/**",
        "**/.eslint*",
        "**/tsconfig.json"
      ],
      "output": []
    },
    "test:compile": {
      "command": "tsc -p \"./test\" --pretty",
      "files": [
        "test/**/*.ts",
        "**/tsconfig.json"
      ],
      "output": []
    },
    "test": {
      "dependencies": [
        "test:deprecation-policy",
        "lint",
        "test:json-schema"
      ]
    },
    "test:only": {
      "command": "c8 mocha \"test/**/*test.ts\"",
      "env": {
        "FORCE_COLOR": "2"
      },
      "files": [
        "test/**/*.ts",
        "src/**/*.ts",
        "**/tsconfig.json",
        ".mocha*",
        "!*.nut.ts",
        ".nycrc"
      ],
      "output": []
    },
    "test:command-reference": {
      "command": "\"./bin/dev.js\" commandreference:generate --erroronwarnings",
      "files": [
        "src/**/*.ts",
        "messages/**",
        "package.json"
      ],
      "output": [
        "tmp/root"
      ]
    },
    "test:deprecation-policy": {
      "command": "\"./bin/dev.js\" snapshot:compare",
      "files": [
        "src/**/*.ts"
      ],
      "output": [],
      "dependencies": [
        "compile"
      ]
    },
    "test:json-schema": {
      "command": "\"./bin/dev.js\" schema:compare",
      "files": [
        "src/**/*.ts",
        "schemas"
      ],
      "output": []
    }
  }
}
